{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "DynamicAzure"
		},
		"Azdemo_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Azdemo'"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"AzureSqlDatabasesrc_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabasesrc'"
		},
		"AzureSqlDatabasetarget_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabasetarget'"
		},
		"AzureSqlDatabasetargetincre_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabasetargetincre'"
		},
		"DynamicBlob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'DynamicBlob'"
		},
		"DynamicTables_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'DynamicTables'"
		},
		"SalesAzureBlobStorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SalesAzureBlobStorage'"
		},
		"Target_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Target'"
		},
		"demoblob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'demoblob'"
		},
		"linkedinc_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'linkedinc'"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Azdemo_connectionString": {
						"value": "[parameters('Azdemo_connectionString')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"AzureSqlDatabasesrc_connectionString": {
						"value": "[parameters('AzureSqlDatabasesrc_connectionString')]"
					},
					"AzureSqlDatabasetarget_connectionString": {
						"value": "[parameters('AzureSqlDatabasetarget_connectionString')]"
					},
					"AzureSqlDatabasetargetincre_connectionString": {
						"value": "[parameters('AzureSqlDatabasetargetincre_connectionString')]"
					},
					"DynamicBlob_connectionString": {
						"value": "[parameters('DynamicBlob_connectionString')]"
					},
					"DynamicTables_connectionString": {
						"value": "[parameters('DynamicTables_connectionString')]"
					},
					"SalesAzureBlobStorage_connectionString": {
						"value": "[parameters('SalesAzureBlobStorage_connectionString')]"
					},
					"Target_connectionString": {
						"value": "[parameters('Target_connectionString')]"
					},
					"demoblob_connectionString": {
						"value": "[parameters('demoblob_connectionString')]"
					},
					"linkedinc_connectionString": {
						"value": "[parameters('linkedinc_connectionString')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ArmTemplate_0')]"
			]
		}
	]
}